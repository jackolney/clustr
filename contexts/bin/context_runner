#!/usr/bin/env Rscript
# autogenerated by context; do not edit
library(methods)
message("args:")
message(paste(commandArgs(), collapse=", "))
local({
main_parse_args <- function (args) 
{
    nargs <- length(args)
    usage <- "context <root> <task_id>"
    if (nargs != 2L) {
        stop("Exactly two arguments required\nUsage: ", usage, 
            "(given: ", paste(args), ")", call. = FALSE)
    }
    list(root = args[[1]], id = args[[2]])
}
args <- main_parse_args(commandArgs(TRUE))
CONTEXT_ROOT <- args$root
source(file.path(CONTEXT_ROOT, "bin", "context_bootstrap"), TRUE)
})
context:::main()
